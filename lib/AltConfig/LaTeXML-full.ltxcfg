#  -*- mode:yaml -*-
provides:
  - Unified core and post LaTeXML processing
  - Default configuration for daemonized execution of LaTeXML
dependencies:
  - LaTeXML-core
  - LaTeXML-post

defaults:
  # Daemon, which happens to be global:
  whatsin: document
  whatsout: document
  input_limit: 1000 # 1000 documents
  expire: 60
  address: 127.0.0.1'
  port: 3334